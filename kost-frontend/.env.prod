# React Frontend Production Environment

# Build mode
NODE_ENV=production

# API Configuration
VITE_API_URL=https://your-domain.com/api
VITE_APP_URL=https://your-domain.com

# App Configuration
VITE_APP_NAME="Potuna Kost"
VITE_APP_VERSION="1.0.0"

# Environment
VITE_NODE_ENV=production

# WebSocket/Broadcasting
VITE_PUSHER_APP_KEY=your_pusher_app_key
VITE_PUSHER_APP_CLUSTER=ap1
VITE_PUSHER_HOST=
VITE_PUSHER_PORT=443
VITE_PUSHER_SCHEME=https

# Third-party Services
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key
VITE_GOOGLE_ANALYTICS_ID=your_google_analytics_id

# MQTT Configuration (for IoT features)
VITE_MQTT_HOST=your-domain.com
VITE_MQTT_PORT=9001
VITE_MQTT_USE_SSL=true

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_PWA=true

# SEO Configuration
VITE_SITE_TITLE="Potuna Kost - Hunian Nyaman di Jakarta Selatan"
VITE_SITE_DESCRIPTION="Kos nyaman dan strategis di Jagakarsa, Jakarta Selatan untuk mahasiswa dan pekerja"
VITE_SITE_KEYWORDS="kost,kos,jakarta selatan,jagakarsa,hunian,mahasiswa"

# Contact Information
VITE_WHATSAPP_NUMBER=6281234567890
VITE_PHONE_NUMBER=6281234567890
VITE_EMAIL=info@potunakos.com